<?xml version="1.0" encoding="utf-8"?>
<commands xmlns="http://schemas.foxone.com/netframework/sqlcommand">
  <command key="Security.GetUserLoginInfo">
    <![CDATA[
        SELECT * FROM SYS_User  WHERE 1=1
        {? and Loginid = #LoginId# }
        {? or Age>#Age# }
    ]]>
  </command>
</commands>