@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="renderer" content="webkit" />
    <meta name="renderer" content="ie-stand" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>绑定用户账号</title>
    @System.Web.Optimization.Styles.Render("~/style/common.css")


    <style>
        h4 {
            font-size: 16px;
            margin: 10px 0;
            color: #0784de;
        }

        code {
            padding: 2px 4px;
            font-size: 90%;
            color: #c7254e;
            background-color: #f9f2f4;
            border-radius: 4px;
        }

        .demo-box {
            margin: 10px 0;
            padding: 15px;
            border: 1px solid #0784de;
            border-radius: 3px;
        }

        p {
            margin: 5px 0;
        }

        .content-title {
            height: 47px;
            line-height: 47px;
            padding: 0 0 0 16px;
            margin: 0 0 20px 16px;
            font-size: 18px;
            color: #676767;
            font-weight: normal;
            border-bottom: 1px solid #ddd;
        }
                .error-message {
            color: #fd2400;
            font-size: 12px;
            padding-right: 10px;
        }
    </style>
</head>
<body>
    <div style="margin: 20px;">

        <h2 class="content-title">用户绑定成功！</h2>

    </div>
    @System.Web.Optimization.Scripts.Render("~/script/common.js")
</body>
</html>